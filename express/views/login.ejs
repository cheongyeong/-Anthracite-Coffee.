<main class="login_main">

  <div class="loginPage_wrap">
    <div class="loginPage_Img swiper mySwiper5">
      <div class="loginPage_logo"><a href="/main"></a></div>
      <div class="swiper-wrapper">
        <div class="swiper-slide">
          <img src="img/login_slide2.jpg" alt="">
        </div>
        <div class="swiper-slide">
          <img src="img/login_slide4.jpg" alt="">
        </div>
        <div class="swiper-slide">
          <img src="img/login_slide3.jpg" alt="">
        </div>

      </div>
    </div>
    <div class="loginBox_wrap">
      <div class="flex">
        <div class="idpw_box">
          <h1 class="login_Title">로그인</h1>
          <input class="id_box" type="text" placeholder="아이디">
          <input class="pw_box" type="password" placeholder="비밀번호">
          <input class="loginBtn" type="submit" value="로그인">
          <div class="login_subBtn">
            <button class="joinBtn" id="showUp" onclick="location.href='/join'">회원가입</button><span></span>
            <input class="findIdBtn noto" type="submit" value="아이디찾기"><span></span>
            <input class="findPwBtn noto" type="submit" value="비밀번호찾기">
          </div>
        </div>
        <div class="login_sns">
          <div class="login_naver"><span></span>네이버로 시작하기</div>
          <div class="login_kakao"><span></span>카카오로 시작하기</div>
          <div class="login_google"><span></span>구글로 시작하기</div>
          <div class="login_facebook"><span></span>페이스북으로 시작하기</div>
        </div>
      </div>
      <div class="loginBox_logo"></div>
    </div>
  </div>
  </div>
  </div>
  </section>


</main>
<script src="https://cdn.jsdelivr.net/npm/swiper/swiper-bundle.min.js"></script>

<!-- Initialize Swiper -->
<script>
  var swiper = new Swiper(".mySwiper5", {
    spaceBetween: 30,
    centeredSlides: true,
    autoplay: {
      delay: 3000,
      disableOnInteraction: false,
    },

  });
</script>