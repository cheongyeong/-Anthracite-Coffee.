<article class="formContents_page">
  <div class="contentsPage_header">
    <div>
      <span name="id"><%= row.id %></span>
      <span class="contentsPage_HeaderTitle"><span class="board_lock"></span><%= row.noticeTitle %></span>
      <span class="contentsPage_HeaderName" name="writer">관리자</span>
      <span class="contentsPage_HeaderDate" name="create_time"><%= row.create_time %></span>
    </div>
    </tr>
    </thead>
  </div>
  <div class="contentsPage_body">
    <p class="contentsPage_text" name="textBox"><%= row.noticeContent %>

    </p>
  </div>
  <input class="listBtn" type="button" value="목록" onclick="location.href='/contact'">
  <div class="form_Buttons">
    <input class="editBtn" type="button" value="수정" onclick="javascript:location.href='/notice_edit?id=<%= row.id %>'">
    <input class="deleteBtn" type="submit" value="삭제"
      onclick="javascript:location.href='/deleteNotice?id=<%= row.id %>'">
  </div>

</article>