<main class="noticePage">
  <div class="noticePage-img">
    <div class="noticelogo_wrap">
      <h1 class="logo_white"><a href="/"></a></h1>
      <h2>공지사항</h2>
    </div>
  </div>
  <nav class="shop_submenu">
    <ul>
      <li><a href="/notice" style="text-decoration:underline;">공지사항</a></li>
      <li><a href="/contact">샘플 신청 안내</a></li>
    </ul>
  </nav>

  <section class="notice-wrap">
    <h2 class="show-Title">공지사항</h2>
    <div class="sampleRequest_listWrap">

      <table class="request_list_wrap">
        <thead class="list_header">
          <tr>
            <th class="list_header_number">No</th>
            <th class="list_header_title">제목</th>
            <th class="list_header_written">글쓴이</th>
            <th class="list_header_date">작성일</th>
          </tr>
        </thead>
        <tbody class="list_body">
          <% rows.forEach((row, index)=>{ %>
          <tr class="list_line">
            <td><%= row.id %></td>
            <td class="list_body_title">
              <a onclick="javascript:location.href='/notice_detail?id=<%= row.id %>'">
                <%= row.noticeTitle %>
              </a>
            </td>
            <td>관리자</td>
            <td><%= row.create_time %></td>
            <td><input class="notice-editBtn" type="button" value="수정"
                onclick="javascript:location.href='/notice_edit?id=<%= row.id %>'">
            <td><input class="notice-deleteBtn" type="button" value="X"
                onclick="javascript:location.href='/deleteNotice?id=<%= row.id %>'">
          </tr>
          <% }); %>

        </tbody>
      </table>
      <div class="list_top_wrap">
        <input class="writeBtn" type="button" value="작성" onclick="location.href='/notice_write'"></a></input>

        <!-- <input class="search_box" type="text" placeholder="search"> -->
      </div>


    </div>

  </section>


</main>